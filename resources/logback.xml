<configuration debug="true">
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d{ISO8601}] %-5level [%thread] %logger{15} - %msg%n
            </pattern>
        </encoder>
    </appender>


    <logger name="com.peterjurkovic.demo" level="debug" />
    
    <logger name="ratpack" level="debug" />

    <root level="WARN">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>